generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime

  @@map("sessions")
}

model User {
  id        Int    @id @default(autoincrement())
  email     String @unique
  firstName String @map("first_name") @db.VarChar(25)
  lastName  String @map("last_name") @db.VarChar(25)
  password  String

  @@map("users")
}
